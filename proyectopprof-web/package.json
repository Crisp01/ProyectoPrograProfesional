  "scripts": {
    "clean:all": "sh -c \"find . -type d \\\\( -name 'node_modules' -o -name '.next' \\\\) -prune -exec rm -rf {} + && rm -f next-env.d.ts *.tsbuildinfo deno.lock package-lock.json\"",
    "install": "deno install --allow-scripts",
    "update": "deno outdated --update",
    "format": "prettier --write .",
