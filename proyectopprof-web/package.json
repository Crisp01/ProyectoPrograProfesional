    "@mui/material-nextjs": "^7.1.1",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.16.0",
    "next": "15.3.3",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
